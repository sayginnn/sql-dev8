--Sütunların her birine göre diğer sütunları güncelleyecek 5 adet UPDATE işlemi yapalım.


UPDATE employee
SET name = 'sena'
WHERE id < 58;
UPDATE employee 
SET birthday = '1997-01-11'
WHERE id =3;
UPDATE employee 
SET name = 'buse'
WHERE id = 19;
UPDATE employee 
SET birthday= '2022-04-17'
WHERE name ILIKE 'A%';
UPDATE employee 
SET name = 'aynur'
WHERE id = 45;
--Sütunların her birine göre ilgili satırı silecek 5 adet DELETE işlemi yapalım

delete from employee
where name='sena'
returning *;
delete from employee
where birthday='2022-04-17'
returning *;
delete from employee
 WHERE id =3
returning *;
delete  from employee
 WHERE name like '%D'
returning *;
delete  from employee
 WHERE id =1
returning *;
